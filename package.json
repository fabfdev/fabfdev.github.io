{
  "name": "fabfdev",
  "version": "1.0.0",
  "main": "index.ts",
  "homepage": "https://fabfdev.github.io",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo export --platform web && node fix-paths.js",
    "predeploy": "npm run build",
    "deploy": "cp -r dist/* . && git add index.html favicon.ico metadata.json _expo/ && git commit -m 'Deploy React app to GitHub Pages' && git push origin main"
  },
  "dependencies": {
    "expo": "~53.0.13",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.4",
    "react-dom": "19.0.0",
    "react-native-web": "^0.20.0",
    "@expo/metro-runtime": "~5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "gh-pages": "^6.3.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
